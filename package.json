{
  "dependencies": {
    "@google/generative-ai": "^0.2.1",
    "@neondatabase/serverless": "^0.9.3",
    "@supabase/supabase-js": "^2.43.4",
    "@tensorflow-models/universal-sentence-encoder": "^1.3.3",
    "@tensorflow/tfjs": "^4.20.0",
    "@tensorflow/tfjs-node": "^4.20.0",
    "compromise": "^14.13.0",
    "discord.js": "^14.15.3",
    "dotenv-flow": "^4.1.0",
    "drizzle-orm": "^0.31.2",
    "node-fetch": "^3.3.2",
    "pg": "^8.12.0",
    "postgres": "^3.4.4",
    "supabase": "^1.172.2",
    "word2vec": "^1.1.5"
  },
  "name": "discord-bot",
  "type": "module",
  "version": "1.0.0",
  "main": "src/index.ts",
  "scripts": {
    "dev": "NODE_ENV=development tsx ./src/index.ts",
    "test": "NODE_ENV=development vitest",
    "clean": "rimraf dist",
    "compile": "tsc --project tsconfig.json --outDir dist",
    "build": "npm run clean && npm run compile && vite build",
    "start": "node dist/index.js",
    "generate": "npm run generate:dev",
    "generate:dev": "NODE_ENV=development drizzle-kit generate --config ./src/db/drizzle.config.ts",
    "generate:prod": "NODE_ENV=production drizzle-kit generate --config ./src/db/drizzle.config.ts",
    "migrate": "npm run migrate:dev",
    "migrate:dev": "NODE_ENV=development drizzle-kit migrate --config ./src/db/drizzle.config.ts",
    "migrate:prod": "NODE_ENV=production drizzle-kit migrate --config ./src/db/drizzle.config.ts",
    "start-droplet": "npm run start:prod",
    "start:prod": "NODE_ENV=production node dist/index.js"
  },
  "keywords": [],
  "author": "gamb1t",
  "license": "ISC",
  "description": "A discord bot that uses natural language processing to answer user questions",
  "devDependencies": {
    "@rollup/plugin-commonjs": "^26.0.1",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@sveltejs/vite-plugin-svelte": "^3.1.1",
    "drizzle-kit": "^0.22.7",
    "nodemon": "^3.1.3",
    "path": "^0.12.7",
    "tsc-alias": "^1.8.10",
    "tsx": "^4.15.6",
    "typescript": "^5.4.5",
    "vite": "^5.3.3",
    "vite-plugin-static-copy": "^1.0.6",
    "vitest": "^1.6.0"
  }
}
